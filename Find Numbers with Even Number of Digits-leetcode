class Solution {
public:
    int findNumbers(vector<int>& nums) {
        int rem,dig,evencount;
        for(int i=0;i<nums.size();i++){
            dig=0;
            while(nums[i]>0){
                rem=nums[i]%10;
                nums[i]=nums[i]/10;
                dig++;
                
            }
            if(dig%2==0){
                evencount++;
            }
            
        }
        return evencount;
    }
};
